<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>AdminLogin</servlet-name>
        <servlet-class>admin.AdminLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BroadcastNot</servlet-name>
        <servlet-class>admin.BroadcastNot</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Settings</servlet-name>
        <servlet-class>admin.Settings</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Newserv</servlet-name>
        <servlet-class>admin.Newserv</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ScheduleNot</servlet-name>
        <servlet-class>broadcast.ScheduleNot</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServ</servlet-name>
        <servlet-class>admin.LogoutServ</servlet-class>
    </servlet>
    <!--    <servlet>
            <servlet-name>UploadServlet</servlet-name>
            <servlet-class>admin.UploadServlet</servlet-class>
        </servlet>-->
    <servlet>
        <servlet-name>Deletion</servlet-name>
        <servlet-class>notifications.Deletion</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AdminLogin</servlet-name>
        <url-pattern>/AdminLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BroadcastNot</servlet-name>
        <url-pattern>/BroadcastNot</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Settings</servlet-name>
        <url-pattern>/Settings</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Newserv</servlet-name>
        <url-pattern>/Newserv</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ScheduleNot</servlet-name>
        <url-pattern>/ScheduleNot</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServ</servlet-name>
        <url-pattern>/LogoutServ</url-pattern>
    </servlet-mapping>
    <!--    <servlet-mapping>
            <servlet-name>UploadServlet</servlet-name>
            <url-pattern>/UploadServlet</url-pattern>
        </servlet-mapping>-->
    <servlet-mapping>
        <servlet-name>Deletion</servlet-name>
        <url-pattern>/Deletion</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            7200
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
